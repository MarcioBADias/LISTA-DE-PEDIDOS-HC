<h1>Pedidos Realizados</h1>
<ul class="form">
    <div>
        <a href="/"><button>Voltar</button></a>
        <a href="/addOrder" class="showOrders"><button>Cadastrar Pedido</button></a>
    </div>
    {{#each orders}}
    <li>
        #{{this.id}} <strong>{{this.holder}}</strong>: <span class="edition">{{this.edition}}</span> 
        <a href="/orders/{{this.order}}">{{this.order}}</a>
    </li>
    {{/each}}
    <div>
        <a href="/"><button>Voltar</button></a>
        <a href="/addOrder" class="showOrders"><button>Cadastrar Pedido</button></a>
    </div>
</ul>